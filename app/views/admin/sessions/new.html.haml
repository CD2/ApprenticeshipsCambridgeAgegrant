= flash.to_a

= form_for :session, url: admin_login_path do |f|
  .field
    = f.label :email
    = f.email_field :email, placeholder: "Email"
  .field
    = f.label :password
    = f.password_field :password, placeholder: "Password"
  .actions
    = link_to "Forgot password?", new_password_reset_path
    = f.submit "Log in"
